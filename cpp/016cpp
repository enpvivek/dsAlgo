#include <bits/stdc++.h>
using namespace std;

int chefAndDolls(int arr[], int n) {
  int res = 0;
  for (int i = 0; i < n; i++) {
    res = res ^ arr[i];
  }
  return res;
}

int main() {
  int t;
  cin >> t;
  while (t--) {
    int n;
    cin >> n;
    int arr[n];
    for (int i = 0; i < n; i++) {
      cin >> arr[i];
    }
    cout << chefAndDolls(arr, n) << endl;
  }

  return 0;
}
